{
	"manifest_version": 2,

	"name": "Group Tabs",
	"description": "Chrome extension inspired by Firefox tab group extension",
	"version": "0.1.3",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
	"default_title": "Group Tabs"
	},
	"background": {
		"scripts": ["tab_group.js", "background.js"]
	},
	"permissions": [
		"activeTab","tabs", "storage"
	]
}